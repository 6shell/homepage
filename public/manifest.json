{
  "name": "howdy-dashboard",
  "version": "0.0.1",
  "description": "A home start page for customing your personal dashboard from some components.",
  "icons": {
    "128": "favicon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "favicon.png"
  },
  "offline_enabled": true,
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}