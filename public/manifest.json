{
  "name": "Howdz起始页",
  "version": "0.0.1",
  "description": "提供大量组件用于定制化你的起始页，可适配响应式设计。",
  "icons": {
    "128": "favicon.png"
  },
  "permissions": ["tabs", "activeTab", "webRequest"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "favicon.png"
  },
  "offline_enabled": true,
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
